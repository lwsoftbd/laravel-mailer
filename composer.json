{
    "name": "lwsoftbd/laravel-mailer",
    "description": "Database driven SMTP manager for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mithun Sarker",
            "email": "dev@lwsoftbd.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12",
        "illuminate/support": "^9|^10|^11|^12"
    },
    "autoload": {
        "psr-4": {
            "Lwsoftbd\\LaravelMailer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lwsoftbd\\LaravelMailer\\LaravelMailerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
