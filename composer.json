{
    "name": "lwsoftbd/lw-mailer",
    "description": "A Laravel Mailer Package for Managing Site Email and SMTP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mithun Sarker",
            "email": "dev@lwsoftbd.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/database": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "LWSoftBD\\LwMailer\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWSoftBD\\LwMailer\\LwMailerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
